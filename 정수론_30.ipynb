    "n = sorted(n, reverse = True)\n",
    "\n",
    "\n",
    "if '0' not in n:\n",
    "    print(-1)\n",
    "else:\n",
    "    temp = 0\n",
    "    for i in n:\n",
    "        temp += int(i)\n",
    "    if temp % 3 == 0:\n",
    "        print(''.join(n))\n",
    "    else:\n",
    "        print(-1)"
